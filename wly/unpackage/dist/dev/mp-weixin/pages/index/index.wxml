<view><view class="tittle center bold">学生免费流量申请表</view><view class="time center"><view class="bold">{{'申请时间：'+date+''}}</view></view><view class="excel-tittle center bold">申请人基本信息</view><view><form data-event-opts="{{[['submit',[['formSubmit',['$event']]]]]}}" bindsubmit="__e"><view class="card"><view class="interval bold">{{'认证账号:'+id+''}}</view></view><view class="card"><view class="interval bold">{{'校园卡号：'+cardnumber+''}}</view></view><view class="card"><view class="interval bold">{{'性别：'+gender+''}}</view></view><view class="card"><view class="interval bold">{{'学院：'+department+''}}</view></view><view class="card"><view class="interval bold">专业：</view><input name="学院" placeholder="请输入专业" data-event-opts="{{[['input',[['__set_model',['','project','$event',[]]]]]]}}" value="{{project}}" bindinput="__e"/></view><view class="card"><view class="interval bold">宿舍地址：</view><input name="宿舍地址" placeholder="请输入宿舍地址" data-event-opts="{{[['input',[['__set_model',['','dorm','$event',[]]]]]]}}" value="{{dorm}}" bindinput="__e"/></view><view class="card"><view class="interval bold">联系电话：</view><input name="联系电话" placeholder="请输入联系电话" data-event-opts="{{[['input',[['__set_model',['','phonenumber','$event',[]]]]]]}}" value="{{phonenumber}}" bindinput="__e"/></view><view class="card"><view class="interval bold">辅导员姓名：</view><input name="辅导员姓名" placeholder="请输入辅导员姓名" data-event-opts="{{[['input',[['__set_model',['','teacherName','$event',[]]]]]]}}" value="{{teacherName}}" bindinput="__e"/></view><view class="card"><view class="interval bold">辅导员联系电话：</view><input name="辅导员联系电话" placeholder="请输入辅导员联系电话" data-event-opts="{{[['input',[['__set_model',['','teacherPhonenumber','$event',[]]]]]]}}" value="{{teacherPhonenumber}}" bindinput="__e"/></view><view class="card"><view class="interval bold">申请额度：</view><radio-group bindinput="__e" class="radio-card" name="申请额度" value="{{howMuch}}" data-event-opts="{{[['input',[['__set_model',['','howMuch','$event',[]]]]]]}}"><label><radio value="0"></radio><text class="radio">10G</text></label><label><radio value="1"></radio><text class="radio">30G</text></label><label><radio value="2"></radio><text class="radio">50G</text></label></radio-group></view><view class="card"><view class="interval bold">申请原因简述</view><input name="申请原因简述" placeholder="请简述申请原因" data-event-opts="{{[['input',[['__set_model',['','reason','$event',[]]]]]]}}" value="{{reason}}" bindinput="__e"/></view><view class="card"><view class="interval bold">申请相关附件</view><uni-section vue-id="8dd740cc-1" title="选择相关附件" bind:__l="__l" vue-slots="{{['default']}}"><uni-file-picker vue-id="{{('8dd740cc-2')+','+('8dd740cc-1')}}" file-mediatype="all" list-styles="{{listStyles}}" data-event-opts="{{[['^progress',[['progress']]],['^success',[['success']]],['^fail',[['fail']]],['^select',[['select']]]]}}" bind:progress="__e" bind:success="__e" bind:fail="__e" bind:select="__e" bind:__l="__l"></uni-file-picker></uni-section></view><view><button form-type="submit">提交</button></view></form></view></view>